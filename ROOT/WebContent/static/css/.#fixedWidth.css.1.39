}
#pickup_note {
	font-size: 12px;
	line-height: 12px;
	height: 13px;
	min-height: 13px;
	max-height: 13px;
	white-space: nowrap;
	overflow: hidden;
	width: 170px;
	text-overflow: ellipsis;
	word-wrap: break-word;
	color: #AF3E00;
	border: 1pt dashed white;
	border-bottom: 1pt dashed #eaeaea;
	/*text-decoration: underline;*/
	font-style: italic;
	cursor: pointer;
	/*background: url(../images/pointer2.gif) top right no-repeat;*/

}

