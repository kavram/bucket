}
#details {
	font-size: 10px;
	text-decoration: underline;
	font-weight: normal;
	cursor: pointer;
	position: relative;
	top: 0px;
	left: 0px;
	padding-left: 5px;
	padding-right: 15px;
}
.offerHeaderCell, .offerHeaderTextCell {
	display: table-cell;
	border: 1pt solid transparent;
	font-weight: bold;
	vertical-align: middle;
	text-align: left;
	height: 17px;
	white-space: nowrap;
	color: #3B5278;
	font-size: 12px;
}
.offerHeaderCell {
	width: 70px;
}
.offerHeaderTextCell {
	color: #8C1717;
