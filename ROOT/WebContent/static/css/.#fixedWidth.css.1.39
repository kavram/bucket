	border: 1px solid #3B5278;
	border-spacing: 5px;
}
#customerNameTable {
	display: table;
	margin: 0 auto;
	width: 98%;
	border-spacing: 0px;
}
.cell #name {
	color: #3B5278;
	border-bottom: 1pt dashed #3B5278;
}
.cell #custLabel {
	font-size: 13px;
	width: 76px;
	text-align: left;
}
.cell #custName {
	font-size: 13px;
	font-weight: bold;
	text-align: left;
}
.offerCell {
	display: table-cell;
	border: 1pt solid #d9d6c4;
	vertical-align: top;
	text-align: left;
	height: 19px;
	font-size: 12px;
	padding: 1px;
	margin: 0px;
}
.offerCellAmount {
	display: table-cell;
