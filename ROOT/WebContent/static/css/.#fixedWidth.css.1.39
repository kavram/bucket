	border-radius: 3px;
	table-layout: fixed;
	/*background: #eaeaea url(../images/smudge-pale-green.gif) 0% 0% repeat;*/
}
.offerGrid, .offerHeader {
	display: table;
	border-collapse: collapse;
	border: 1px solid transparent;
	width: 100%;
	/*border-spacing: 2pt;*/
}
#itemsTblCell {
	padding: 0px 2px 0px 3px;
}
