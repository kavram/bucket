.wrapper {
	width: 980px;
	margin: 0 auto;
	text-align: left;
	/*background-color: #EBEBEB;*/
	min-height: 600px;
}
.header {
	width: 100%;
	box-shadow: 3px 3px 6px #777;
	height: 50px;
	background-color: #fff;
	-moz-border-radius: 3px;
	border-radius: 3px;
	text-align: right;
	position: relative;
}
.footer {
	width: 100%;
	display: block;
	box-shadow: 3px 3px 6px #777;
	background-color: #FFF;
	height: 50px;
	min-height: 50px;
	text-align: center;
	font-size: 0.9em;
	color: #CAD5E7;
	margin-bottom: 4px;
	-moz-border-radius: 3px;
	border-radius: 3px;
}
.content {
	width: 100%;
	display: block;
	overflow: hidden;
	background-color: #FFF;
	box-shadow: 3px 3px 6px #777;
	/*padding: 3px;*/
	min-height: 400px;
	/*height: 500px;*/
	-moz-border-radius: 3px;
	border-radius: 3px;
	margin: 10px 0px 10px 0px;
}
.account {
	width: 70%;
	position: relative;
	top: 30px;
	left: 27%;
	border: 1px solid transparent;
	font-size: 11px;
}
.account a:link {
	color: #000000;
	padding-right: 10px;
}
.account a:visited {
	color: #000000;
}
.account a:hover {
	color: #000000;
}
.account a:active {
	color: #000000;
}
.account #home {
	background: url(../images/home.png) left top no-repeat;
	padding-left: 17px;
	margin-right: 12px;
}
.gridContent {
	display: table;
	border-collapse: separate;
	border-spacing: 1px;
	width: 100%;
	min-width: 100%;
	overflow-y: scroll;
	height: 100%;
	table-layout: fixed;
	margin: 0 auto;
	/*-moz-border-radius: 3px;
	 border-radius: 3px;
	 border: 1px solid #d9d6c4;*/
}
.gridHeader {
	/*-moz-border-radius: 3px;
	 border-radius: 3px;
	 border: 1px solid #d9d6c4;
	 border-bottom: none;*/
	text-align: left;
	height: 100%;
	position: relative;
	top: 3px;
	left: 3px;
	color: #3B5278;
	font-size: 12px;
	font-weight: bold;
}
.gridHeader #orderCaption {
	text-align: left;
	height: 100%;
	position: relative;
	top: 3px;
	left: 0px;
}

#offersCtn {
	overflow: hidden;
	margin: 0 auto;
	width: 70%;
	padding: 5px;
}
.row {
	display: table-row;
}
.caption {
	caption-side: top; 
    width: auto;
    text-align: center;	
    display: table-caption;
	margin-bottom: 20px;
	margin-top: 20px;
	font-size: 12px;
}
#tblItemsDlg {
	display: table;
	border-collapse: collapse;
	border: 1px solid transparent;
	width: 99%;	
	font-family: Verdana, Trebuchet MS, Tahoma, Arial, sans-serif;
	font-size: 70%;	
}
.cell {
	display: table-cell;
	border: 1pt solid transparent;
	vertical-align: top;
	text-align: center;
	/*padding-bottom: 850px;
	 margin-bottom: -850px;*/
}
.leftContent {
	padding-left: 3px;
	width: 60%;
	min-width: 350px;
	text-align: left;
}
.signinLeftContent {
	padding-left: 60px;
	padding-top: 20px;
}
.rightContent {
	padding-right: 3px;
	width: 40%;
	min-width: 350px;
	text-align: right;
}
